procedure main()
   move5 := newmove(5)   # A matching co-expression
   move4 := newmove(4)
  s:= "Now is the time for all good men to come to the aid" 
  s ?  while write(move5()) do move4()

end

procedure newmove(n)
  return create |(move(n)\1)
end