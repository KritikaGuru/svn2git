procedure main()
    write("loading level2")
    if (x := load("level2")) then write("loaded ", image(x))
    else stop("couldn't load(level2)")

   if &level=1 then
      write("source program &level returned 1. This is Correct!")
   else
      write("source program &level returned ", &level ,  " This is WRONG!!")
      
   foo()

   
    write(" keword(): ",keyword("level", x))
   
    write("running level2")
    @x
    write("returned to parent.")
    write(" keword(): ",keyword("level", x))
    
   if &level=1 then
      write("source program &level returned 1. This is Correct!")
   else
      write("source program &level returned ", &level ,  " This is WRONG!!")
      
   foo()
   
   write("DONE!  goodbye.")

end

procedure foo()
   if &level=2 then
      write("source program &level returned 2. This is Correct!")
