procedure main()
   out := &output
   s := "abcda"
   if s ?? Pos(1) || "b" then
      write("Match succeeded")
   else
      write("Match failed")
   s ?? Len(3) -> out || Rpos(0) 
   s ?? Pos(4) || Len(1) -> out
   if s ?? Pos(1) || "abcd" || Rpos(0) then
      write("Match succeeded")
   else
      write("Match failed")
   out := &output
   s := "abcda"
   s ?? Len(3) -> out
   s ?? Len(2) -> out || "a"
end
