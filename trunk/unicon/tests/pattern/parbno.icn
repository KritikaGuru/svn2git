# parbno.icn
# Tests: Pos(), Arbno(), Rpos(), ??, ||,
#        =>, Span(), included pattern in pattern

procedure main()
   out := &output
   item := Span(&digits) => out
   list := Pos(1) || "(" || item || Arbno("," || item) || ")" || Rpos(0)
   while line := read() do {
      if line ?? list then
         write("Match succeeded: ", line)
      else
         write("Match failed: ", line)
      }
end
