procedure main()
   item := Span(&digits)
   list := Pos(1) && "(" && item && Arbno("," && item) && ")" && Rpos(0)
   if "(12,345,6)" ?? list then
      write("Match succeeded")
   else
      write("Match failed")
   if "(12,,6)" ?? list then
      write("Match succeeded")
   else
      write("Match failed")
end
