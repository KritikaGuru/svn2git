procedure main()
   item := PSpan(&digits)
   list := PPos(1) && "(" && item && PArbno("," && item) && ")" &&
      PRpos(0)
   if "(12,345,6)" ?? list then
      write("Match succeeded")
   else
      write("Match failed")
   if "(12,,6)" ?? list then
      write("Match succeeded")
   else
      write("Match failed")
end
