procedure test(a,b,c)
	return (a  = (b - a)) & (a = (c - b))
end


procedure main()
	p := PPos(1) && PSpan("a") && .$a && PSpan("b") && .$b && PSpan("c") && .$c && PRpos(0) && `test(a,b,c)`

	while write(line := read()) do
		if (line ?? p) then write(" accepted") 
		else write(" rejected")

	test(1,2,3)
end

