procedure test(a,b,c)
	return (a  = (b - a)) & (a = (c - b))
end

procedure main()
   p := Pos(1) || Span("a") || .> a || Span("b") || .> b ||
	 Span("c") || .> c || Rpos(0) # && `test(a,b,c)` fix

   while line := read() do
      if (line ?? p) & ((a-1) = (b-a) = (c-b))
      then write("accepted") 
      else write("rejected")
end
