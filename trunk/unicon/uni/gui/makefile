base = $(shell dirname `pwd`)
export IPATH:=$(base)/lib

all: libs

clean:
	rm -f *.u uniclass.dir uniclass.pag 

deps:
	$(base)/bin/unidep *.icn -p libs

%.u: %.icn
	unicon -c $*

