class CanvasHSizerUI : BasicCanvasComponentUI()
   method create_canvas_component(canvas)
      t := CanvasSizer()
      t.set_size(100)
      t.set_is_horizontal()
      t.h_default := 1
      return t
   end

   initially()
      self.BasicCanvasComponentUI.initially("Separators", 
                                            "HSizer"
                                            )
end
