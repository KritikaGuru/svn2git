class CanvasTextButtonUI : BasicCanvasComponentUI()
   method create_canvas_component(canvas)
      t := CanvasTextButton()
      t.set_label("Button")
      t.w_default := t.h_default := 1
      return t
   end

   initially()
      self.BasicCanvasComponentUI.initially("Buttons", 
                                            "TextButton",
                                            $include "icon/icn1.icon"
                                            )
end
