class CanvasIconUI : BasicCanvasComponentUI()
   method create_canvas_component(canvas)
      t := CanvasIcon()
      t.set_img(
                $include "icon/icon.icon"
                )
      t.w_default := t.h_default := 1
      return t
   end

   initially()
      self.BasicCanvasComponentUI.initially("Image", 
                                            "Icon",
                                            $include "icon/icn6.icon"
                                            )
end
