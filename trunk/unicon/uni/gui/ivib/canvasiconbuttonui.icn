class CanvasIconButtonUI : BasicCanvasComponentUI()
   method create_canvas_component(canvas)
      t := CanvasIconButton()
      t.set_img(
                $include "icon/icon.icon"
                )
      t.w_default := t.h_default := 1
      return t
   end

   initially()
      self.BasicCanvasComponentUI.initially("Buttons", 
                                            "IconButton",
                                            $include "icon/icn5.icon"
                                            )
end
