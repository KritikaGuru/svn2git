method handle_save_file(filename)  
local file, l
l := []

  l := EditBox.get_contents()

  file := open(filename, "c") | {
     MsgBox.set_contents([filename || ": open failure."])
     fail
  }

  every write(file, !l)
  close(file)

  MsgBox.set_contents([filename || " saved."])
end
